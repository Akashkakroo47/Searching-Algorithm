/*Linear Search*/


#include<bits/stdc++.h>
using namespace std;
void solve()
{
	int n,target;
	int count=0,flag=0;
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	cin>>target;
	for(int i=0;i<n;i++)
	{
		if(arr[i]==target)
		{
			count++;
			flag=1;
			break;
		}
		else
		{
			count++;
		}
	}
	if(flag==1) cout<<"Present"<<" "<<count<<endl;
	else cout<<"Not-Present"<<" "<<count<<endl;
	
}
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		solve();
	}
}
